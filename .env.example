# API Configuration
API_BASE_URL=https://api.bancoademi.com
API_TIMEOUT=30000

# Environment
ENVIRONMENT=development

# Feature Flags
ENABLE_BIOMETRIC_AUTH=true
ENABLE_FACEPHI_VALIDATION=true
ENABLE_ANALYTICS=true
ENABLE_CRASHLYTICS=true

# Security
ENCRYPTION_KEY=your-encryption-key-here
API_KEY=your-api-key-here
CERTIFICATE_PINNING_ENABLED=true

# Firebase (if needed)
# These will be replaced by firebase_options.dart after configuration
# FIREBASE_API_KEY=
# FIREBASE_APP_ID=
# FIREBASE_PROJECT_ID=
# FIREBASE_MESSAGING_SENDER_ID=

# Third-party Integrations
FACEPHI_LICENSE_KEY=your-facephi-license-key
WHATSAPP_NUMBER=+1234567890

# Debugging
DEBUG_MODE=true
LOG_LEVEL=debug

# App Configuration
APP_NAME=Banco Ademi
DEFAULT_LANGUAGE=es
SUPPORTED_LANGUAGES=es,en

# Session Configuration
SESSION_TIMEOUT_MINUTES=15
MAX_LOGIN_ATTEMPTS=3

# OTP Configuration
OTP_LENGTH=6
OTP_EXPIRY_SECONDS=180
